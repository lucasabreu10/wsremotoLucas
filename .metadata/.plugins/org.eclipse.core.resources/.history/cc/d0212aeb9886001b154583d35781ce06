package repeticao;

import javax.swing.JOptionPane;

public class Exercico2 {

	public static void main(String[] args) {
		
		
		String nome = "";
		int idade = 0;
		String cadastro = "";
		int contador = 0;
		int maiores =0;
		
		do {
			contador = contador +1;
			
			nome = JOptionPane.showInputDialog("Digite o nome");
			
			idade = Integer.parseInt(JOptionPane.showInputDialog("Digite a Idade"));
			if(idade > 18) {
				maiores = maiores +1;
			}
			cadastro = JOptionPane.showInputDialog("Se deseja sair digite N ");
			
			
			
			
		} while (cadastro.contains("n") == false);
		
		System.out.println("numero de pessoas " + maiores);
		//System.out.println("numero de pessoas " + );
		
		
		
		
		
		
		
		
	}
	
	
	
}
